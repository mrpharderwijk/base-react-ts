{
  "name": "react-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run build:tailwind:css && react-scripts start",
    "build": "npm run build:tailwind:css && react-scripts build",
    "build:tailwind:css": "postcss src/assets/styles/tailwind.scss -o src/index.scss",
    "build:components:css": "postcss src/App/**/*.scss -o src/assets/styles/components.scss",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "pretty": "prettier --parser typescript --loglevel warn --write '*/**/*.{js,ts,tsx}'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook"
  },
  "precommit": [
    "pretty",
    "lint"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "history": "^5.0.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "storybook": "^5.3.19",
    "typescript": "~3.7.2"
  },
  "devDependencies": {
    "@storybook/react": "^5.3.19",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "autoprefixer": "^9.8.3",
    "enzyme": "^3.11.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "postcss-cli": "^7.1.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.4.6"
  },
  "resolutions": {
    "react-scripts/**/**/yargs-parser": "^18.1.2"
  }
}
